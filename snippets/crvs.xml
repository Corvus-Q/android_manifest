<?xml version="1.0" encoding="UTF-8"?>
<manifest>

<remote name="gh"
        fetch="https://github.com/" />

  <!-- Gitlab remote -->
  <remote  name="gitlab"
           fetch="https://gitlab.com/" />
		
<remote name="rex"
        fetch="https://github.com/Corvus-OS/"
        revision="refs/heads/10" />

  <project path="art" name="android_art" remote="rex" />
  <project path="bionic" name="android_bionic" remote="rex" />
  <project path="bootable/recovery" name="android_bootable_recovery" remote="rex" />
  <project path="platform_testing" name="android_platform_testing" remote="rex" />

  <!-- Build -->
  <project path="build/make" name="android_build" remote="rex" >
    <copyfile src="core/root.mk" dest="Makefile" />
    <linkfile src="CleanSpec.mk" dest="build/CleanSpec.mk" />
    <linkfile src="buildspec.mk.default" dest="build/buildspec.mk.default" />
    <linkfile src="core" dest="build/core" />
    <linkfile src="envsetup.sh" dest="build/envsetup.sh"/>
    <linkfile src="target" dest="build/target"/>
    <linkfile src="tools" dest="build/tools"/>
  </project>

  <project path="build/blueprint" name="android_build_blueprint" remote="rex" />
  <project path="build/soong" name="android_build_soong" remote="rex" >
    <linkfile src="root.bp" dest="Android.bp" />
    <linkfile src="bootstrap.bash" dest="bootstrap.bash" />
  </project>

